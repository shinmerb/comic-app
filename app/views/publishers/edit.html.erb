<h1>編集画面</h1>
<%= form_with model: @publisher, url: publisher_path(@publisher), method: :patch do |form| %>
  <%= render 'form', form: form  %>

  <div>
    <%= form.submit "更新" %>
  </div>
<% end %>

<div class="inline-links">
  <%= link_to "戻る", publisher_path(@publisher), class: "button_other" %>
  <%= link_to "一覧", publishers_path, class: "button_other" %>
</div>
